<script>
function fastOrder() {
  $("#fastorder_error, #fastorder_success").hide();
  $("#fastorderModal .modal-header, #fastorderModal .modal-body").show();

  $("#fastorder_phone").val('');
  $("#fastorder_seller").find('option:first').prop('selected', true);
  $("#fastorder_comment").val('');

  $("#fastorderModal").modal('show');
}

$('#fastorderModal .closeBtn').click(function() {
  $('#fastorderModal').modal('hide');
});

$(document).on('submit', '#fastorder_form', function() {
	let data = new FormData(this);

	data.set('script_load', window.mtaAnalytics !== undefined ? 1 : 0);
	data.set('page_type', '1 click');

	if (window.mtaAnalytics !== undefined) {
		data.set('campaign_js', window.sbjs.get.current.cmp);
		data.set('source_js', window.sbjs.get.current.src);
		data.set('medium_js', window.sbjs.get.current.mdm);
	} else {
		data.set('campaign_js', 'js error');
		data.set('source_js', 'js error');
		data.set('medium_js', 'js error');
	}

	const log = new XMLHttpRequest();

	log.responseType = 'json';
	log.open('POST', '/index.php?route=checkout/analytics/addAnalyticsLog');
	log.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
	log.send(data);

    $("#fastorder_error").hide();

    let submitButton = $(this).find(".buyOneClickBtn");
    submitButton.prop('disabled', true);

	const productId = data.get('product_id');

    $.ajax({
        url: '/index.php?route=extension/module/fastorder/addOrder',
        type: 'POST',
        data: $(this).serialize(),
        dataType: 'json',
        complete: function() {
          submitButton.prop('disabled', false);
        },
        success: function(data) {
          if (data.success) {

            if (typeof window.mtaAnalytics !== 'undefined') {
              window.mtaAnalytics.addOrder(data.order_id);
            }

			if (window.GA4 !== undefined) {
				window.GA4.triggerEvent('order_create', {
					type: 'one click',
                    product_id: productId,
					order_id: data.order_id
				});
			}

            $("#fastorder_success_call").html(data.success_text);
            $("#fastorderModal .modal-header, #fastorderModal .modal-body").hide();
            $("#fastorder_success").show();
          }
          else if(data.error) {
            $("#fastorder_error").text(data.error);
            $("#fastorder_error").show();
          }
        },
        error: function(data) {
          return false;
        }
    });
});

/* $(document).ready(function() {
  $('#fastorder_phone').mask("+38 (099) 999-9999");
}); */
</script>
