<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
        MTAReadyState.push(function ()
        {
            'use strict';

            let valid = new validation();

            $("#forgotPasswordForm").submit(function(e) {
                e.preventDefault();
                let form = $(this);

                let userEmail = $("#forgotEmailInput").val();
                let isEmailValid = valid.email(userEmail);
                let isPhoneValid = valid.phone(userEmail);
                if( !isEmailValid && !isPhoneValid ){
                    $("#forgot_email_error").text('Введіть дійсну адресу електронної пошти (наприклад, name@gmail.com), або телефон (наприклад, 0984051515)');
                    $("#forgotEmailInput").css("border", "1px solid #ff0000");
                } else {
                    $("#forgot_email_error").text('');
                    $("#forgotEmailInput").css("border", "1px solid #ffffff");

                    $.ajax({
                        url: form.attr('action'),
                        type: form.attr('method'),
                        data: form.serialize(),
                        beforeSend: () => {
                            $(".forgotBtnSend").prop( "disabled", true );
                            form.addClass('loader loader_spin');
                        },
                        success: (response) => {
                            if(!response.success){
                                if(response.error_type) {
                                    if(response.error_type == 'email') {
                                        $("#forgotEmailInput").css("border", "1px solid #ff0000");
                                    }

                                    if(response.error) {
                                        $("#forgot_email_error").text(response.error);
                                    }
                                }
                                else {
                                    $("#forgot_email_error").text(response.error);
                                }
                            } else if(response.success === 'code') {
                                $("#forgotAccountPhone").val(response.phone);

                                $("#forgotPasswordForm").hide();
                                $("#forgotCodeForm").show();
                            } else {
                                $("#setsSendOnUserMail").html(userEmail);
                                $("#forgotPasswordForm").css({
                                    "display":"none"
                                });
                                $(".setsForPopupHeadings").css({
                                    "box-shadow": "none",
                                    "background": "none"
                                });
                                $(".setsForPopupHeadings h6").css({
                                    "visibility": "hidden"
                                });
                                $(".forgotPasswordMailSend").css({
                                    "display":"block"
                                });
                            }
                            $(".forgotBtnSend").prop( "disabled", false );
                            form.removeClass('loader loader_spin');
                        },
                        error: (response) => {
                            $(".forgotBtnSend").prop( "disabled", false );
                            form.removeClass('loader loader_spin');
                            alert('System Error!');
                        }
                    });
                }
            });

            $("#forgotCodeForm").submit(function(e) {
                e.preventDefault();
                let form = $(this);

                let phone = $("#forgotAccountPhone");

                if(phone.val().trim() == '') {
                    return false;
                }

                $("#forgot_code_error").text('');
                $("#forgotEmailInput").css("border", "1px solid #ffffff");

                let code = $('#forgotCodeInput');
                let password = $('#forgotNewPasswordInput');
                let confirm = $('#forgotNewConfirmInput');

                let isPasswordValid = valid.password(password.val().trim());

                let error = false;

                if(password.val().trim() != confirm.val().trim()) {
                    $("#forgot_code_error").text('Паролі не співпадають');
                    password.css("border", "1px solid #ff0000");
                    confirm.css("border", "1px solid #ff0000");
                    error = true;
                }

                if(!isPasswordValid) {
                    $("#forgot_code_error").text('Пароль повинен містити мінімум 4 символи');
                    password.css("border", "1px solid #ff0000");
                    error = true;
                }

                if(code.val().trim() == ''){
                    $("#forgot_code_error").text('Необхідно ввести код, який був відправлений на ваш телефон');
                    code.css("border", "1px solid #ff0000");
                    error = true;
                }

                if(error) return true;

                $.ajax({
                    url: form.attr('action'),
                    type: form.attr('method'),
                    data: form.serialize(),
                    success: (response) => {
                        if(response.error) {
                            $("#forgot_code_error").text(response.error);
                        } else if(response.success) {
                            $('.forgotBlock').hide();
                            $('.loginRegisterBlock').show();
                            $('#defaultOpen').click();

                            let html = '<div style="color: #4DAF7C;margin-bottom: 0.5rem;">' + response.text + '</div>';

                            $('#loginForm').prepend(html);
                            /*$("#setsSendOnUserMail").html(userEmail);
                            $("#forgotPasswordForm").css({
                                "display":"none"
                            });
                            $(".setsForPopupHeadings").css({
                                "box-shadow": "none",
                                "background": "none"
                            });
                            $(".setsForPopupHeadings h6").css({
                                "visibility": "hidden"
                            });
                            $(".forgotPasswordMailSend").css({
                                "display":"block"
                            });*/
                        }
                    },
                    error: (response) => {
                        alert('System Error!');
                    }
                });
            });

			let forgot_close = document.querySelector('.forgot-close');

			if (forgot_close) {
			    forgot_close.addEventListener('click', () => {
				    document.querySelector('.closeLoginRegisterPopup').click();
				    document.querySelector('[data-action=\'mta.customer\']').click();
                });
            }
        });
    </script>
