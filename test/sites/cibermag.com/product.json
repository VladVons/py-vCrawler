{
  "product": {
    "info": {
      "author": "vladvons",
      "date": "2024.09.03 09:00:00",
      "url": [
        "https://www.cibermag.com/ua/noutbuki/b-u-noutbuki/dell/noutbuk-dell-latitude-e7250"
      ]
    },
    "pipe_schema": [
      ["app_json", [{"@type": "Product"}]],
      ["as_dict", {
        "name": [
          ["get", ["name"]]
        ],
        "stock": [
          ["gets", ["offers.availability"]],
          ["stock"]
        ],
        "image": [
          ["get", ["image"]]
        ],
        "price": [
          ["ext_price_app", [true]]
        ],
        "category": [
          ["get", ["category"]]
        ]
      }]
    ],
    "pipe_body": [
      ["find", ["div", {"class": "product__container"}]],
      ["as_dict", {
        "name": [
          ["find", ["h1"]],
          ["text"]
        ],
        "stock": [
          ["find", ["div", {"class": "status"}]],
          ["text"],
          ["strip"],
          ["stock"]
        ],
        "price": [
          ["find", ["div", {"class": "price"}]],
          ["text"],
          ["price"]
        ],
        "price_old": [
          ["find", ["div", {"class": "old_price"}]],
          ["text"],
          ["price"]
        ],
        "image": [
          ["ext_image_og"]
        ],
        "images": [
          ["find", ["div", {"class": "product__container__images"}]],
          ["find_all", ["a", {"class": "cm-image-previewer"}]],
          ["list_map", [
            ["get", ["href"]]
          ]]
        ],
        "category": [
          ["var_get", ["$root"]],
          ["find", ["ol", {"class": "breadcrumb"}]],
          ["breadcrumb", [["li", {"itemprop": "itemListElement"}], -2]]
        ],
        "features": [
          ["var_get", ["$root"]],
          ["find", ["div", {"id": "content_features"}]],
          ["table"],
          ["keyval2dict", [0, 1]]
        ],
        "-description": [
          ["find", ["div", {"id": "tabs_content"}]],
          ["text"],
          ["strip"]
        ],
        "brand": [
          ["var_get", ["$features"]],
          ["get", ["Бренд"]]
        ]
      }]
    ]
  }
}
