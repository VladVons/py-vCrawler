{
  "product": {
    "info": {
      "author": "sernuzh",
      "date": "2024.09.06 18:00:00",
      "url": [
        "https://europc.ua/ua/computers/prodesk-600-g2-sff-i3-6100-8-ram-240-ssd/"
      ]
    },
    "pipe_schema": [
      ["app_json", [{"@type": "Product"}]],
      ["as_dict", {
        "name": [
          ["get", ["name"]]
        ],
        "brand": [
          ["gets", ["brand.name"]]
        ],
        "stock": [
          ["gets", ["offers.availability"]],
          ["stock"]
        ],
        "image": [
          ["get", ["image"]]
        ],
        "price": [
          ["gets", ["offers.price"]]
        ],
        "category": [
          ["get", ["category"]]
        ]
      }]
    ],
    "pipe_body": [
      ["find", ["div", {"itemtype": "http://schema.org/Product"}]],
      ["as_dict", {
        "price_old": [
          ["find", ["div", {"class": "price"}]],
          ["find", ["span", {"class": "price-old"}]],
          ["text"],
          ["price"]
        ],
        "images": [
          ["find", ["div", {"class": "images-additional"}]],
          ["find_all", ["a"]],
          ["list_map", [
            ["get", ["href"]]
          ]]
        ],
        "features": [
          ["find", ["div", {"id": "tab-specification"}]],
          ["table"],
          ["keyval2dict", [0, 1]]
        ],
        "description": [
          ["find", ["div", {"id": "tab-description"}]],
          ["text"],
          ["strip"]
        ]
      }]
    ]
  }
}
