{
  "product": {
    "info": {
      "author": "$author",
      "date": "$date",
      "url": [
        "https://your-site1.com/product-page1"
      ]
    },
    "pipe_microdata": [
      ["product_ldjson"],
      ["-product_itemprop"]
    ],
    "pipe_product": [
      ["find", ["div", {"class": "my-product-class"}]],
      ["as_dict", {
        "-images": [
          ["find", ["div", {"class": "more-images"}]],
          ["find_all_get_url", ["a"], {"a_get": "href"}]
        ],
        "-price_old": [
          ["find", ["span", {"class": "price-old"}]],
          ["text_strip"],
          ["price"]
        ]
      }]
    ],
    "pipe_root": [
      ["as_dict", {
        "-description": [
          ["find", ["div", {"itemprop": "description"}]],
          ["text_tag", ["p"]]
        ],
        "-features": [
          ["find", ["div", {"id": "tab-specification"}]],
          ["table"],
          ["keyval2dict"]
        ]
      }]
    ]
  }
}
