{
  "category": {
    "info": {
      "author": "vladvons",
      "date": "2024.10.01 10:00:00",
      "url": [
        "https://evronot.com.ua/catalog/noutbuki_b_u/"
      ]
    },
    "pipe": [
      ["as_dict", {
        "products": [
          ["find", ["div", {"class": "catalog_block"}]],
          ["find_all", ["div", {"class": "item_info"}]],
          ["list_map", [
            ["as_dict", {
               "href": [ 
                 ["find", ["div", {"class": "item-title"}]],
                 ["find", ["a"]],
                 ["get", ["href"]],
                 ["url_pad"]
               ],
               "name": [ 
                 ["find", ["div", {"class": "item-title"}]],
                 ["find", ["a"]],
                 ["text"]
               ],
               "price": [
                 ["find", ["div", {"class": "price"}]],
                 ["text"],
                 ["price"]
              ],
              "stock": [
                 ["find", ["div", {"class": "item-stock"}]],
                 ["text_strip_lower_search", ["есть в наличии"], {"mode": "eq"}]
              ]
            }]
          ]]
       ],
       "pager": [
          ["find", ["ul", {"class": "pagination"}]],
          ["find_all", ["a"]],
          ["list_map", [
            ["get", ["href"]]
          ]],
          ["list_uniq"]
       ]
      }]
    ]
  }
}
