{
  "product": {
    "info": {
      "author": "vladvons",
      "date": "2024.09.19 18:00:00",
      "url": [
        "https://www.gekko-computer.de/Netzwerk/Switch/Infiniband-Switch/Mellanox-MQM8700-HS2F-Quantum-HDR-Edge-Switch-QM8700-40x-200Gbit-QSFP56.html"
      ]
    },
    "pipe_schema": [
      ["app_json", [{"@type": "Product"}]],
      ["as_dict", {
        "name": [
          ["get", ["name"]]
        ],
        "stock": [
          ["gets", ["offers.availability"]],
          ["stock"]
        ],
        "image": [
          ["get", ["image"]]
        ],
        "price": [
          ["ext_price_app"]
        ]
      }]
    ],
    "pipe_body": [
      ["find", ["body", {"class": "product"}]],
      ["as_dict", {
        "images": [
          ["find", ["div", {"id": "thumbs_list"}]],
          ["find_all", ["a", {"class": "thumbnail-no-link"}]],
          ["list_map", [
            ["get", ["href"]]
          ]]
        ],
        "price_old": [
          ["var_get", ["$root"]],
          ["find", ["span", {"id": "old_price_display"}]],
          ["text"],
          ["price"]
        ],
        "description": [
        ],
        "features": [
          ["find", ["div", {"class": "product-features-tab-content"}]],
          ["table"],
          ["keyval2dict", [0, 1]]
        ],
        "brand": [
          ["var_get", ["$features"]],
          ["get", ["Виробник"]]
        ],
        "category": [
          ["find", ["div", {"class": "breadcrumb"}]],
          ["breadcrumb", [["a"], -1, false]]
        ]
      }]
    ]
  }
}
