               "name": [
                 ["find", ["a", {"class": "product-item-link"}]],
                 ["text_strip"]
               ],
               "stock": [
                  ["find_not", ["button", {"class": "tocart"}]],
                  ["invert"]
               ],
               "price": [
                 ["find", ["span", {"class": "price"}]],
                 ["text_strip"],
                 ["price"]
               ]
            }]
          ]]
       ],
       "pager": [
          ["find", ["ul", {"class": "pages-items"}]],
          ["find_all_get_url", ["a"], {"a_get": "href"}]
       ]
      }]
    ]
  }
